maxNum=int(input("Please provide a number more than 1 to create a odd list: "))

for i in range(1,maxNum+1):
    if i%2 != 0:
        print(i)


max = int(input("Enter max number: "))

odd_numbers = []

for i in range(1, max):
    if i % 2 == 1:
        odd_numbers.append(i)

print("Odd numbers: ", odd_numbers)